#include <bits/stdc++.h>
using namespace std;

int main()
{
    //freopen("txt.txt","r",stdin);
    string s;
    int b,l,t;
    cin>>t;
    while(t--)
    {
        cin>>s;
        l=s.length();
        if(l==5)
        {
            cout<<3<<endl;
        }
        else
        {
            if((s[0]=='o' && s[1]=='n')|| (s[1]=='n' && s[2]=='e')  || (s[0]=='o' &&s[2]=='e'))
                cout<<1<<endl;
            else
                cout<<2<<endl;
        }

    }
}
